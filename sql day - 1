1.student table


create table student(
 name varchar(100),
age int,
department varchar(100)
)



#insert values in the table:

insert into student values ("john doe",23,"CSE");
insert into student values("praveen",45,"ECE");
insert into student values ("suresh ",34," ECE");

#update

update student  set department = "computer " where department ="CSE";

#update 

update student set age=30 where name=""john doe"


